{
    "name": "default",
    "model": {
        "which_model": {
            "name": [
                "models.model",
                "Model"
            ],
            "args": {}
        },
        "networks": [
            {
                "name": [
                    "models.cdan",
                    "CDAN"
                ],
                "args": {}
            }
        ]
    },
    "train": {
        "device": "cuda",
        "n_epoch": 100,
        "lr": 1e-3,
        "dataset": {
            "name": [
                "data.dataset",
                "PairedDataset"
            ],
            "args": {
                "low_light_root": "/content/cvccolondbsplit/train/low",
                "normal_light_root": "/content/cvccolondbsplit/train/high"
            }
        },
        "dataloader": {
            "args": {
                "batch_size": 8,
                "shuffle": true,
                "num_workers": 2
            }
        },
        "model_path": "saved_models/",
        "model_name": "CDAN.pt"
    },
    "test": {
        "device": "cuda",
        "dataset": {
            "name": [
                "data.dataset",
                "UnpairedDataset"
            ],
            "args": {
                "low_light_root": "/content/cvccolondbsplit/test/low",
                "image_size": [224,224]
            },
            "is_dataset_paired": false
        },
        "dataloader": {
            "args": {
                "batch_size": 8,
                "shuffle": false,
                "num_workers": 2
            }
        },
        "apply_post_processing": true,
        "model_path": "saved_models/",
        "model_name": "CDAN.pt",
        "output_images_path": "train_images/"
    }
}
